      },
      "package": "com.kelvinmwaijega.Blackmate",
      "permissions": [
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE",
        "MANAGE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "7d6e0560-795c-4f48-8660-7ec8bbeeb72b"
      }
